<?xml version="1.0" encoding="utf-8"?>
<manifest package="checkmobile.de.hertz"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <meta-data
        android:name="com.google.android.gms.vision.DEPENDENCIES"
        android:value="barcode"/>

    <uses-permission android:name="android.permission.CAMERA"/>

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:theme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity">
        </activity>
        <activity android:name=".CarsActivity">
        </activity>
        <activity android:name=".CarInfoActivity">
        </activity>
        <activity android:name=".ProcessesActivity">
        </activity>
        <activity android:name=".DamageActivity">
        </activity>
        <activity android:name=".EmptyActivity">
        </activity>
        <activity android:name=".SelectableListActivity">
        </activity>
        <activity android:name=".TyrePressure">
        </activity>
        <activity android:name=".CaptureMileageActivity">
        </activity>
        <activity android:name=".CaptureFuelActivity">


        </activity>
    </application>

</manifest>