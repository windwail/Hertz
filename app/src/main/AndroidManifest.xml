        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity">
        </activity>
        <activity android:name=".CarsActivity">
        </activity>
        <activity android:name=".CarInfoOverviewActivity">
        </activity>
        <activity android:name=".ProcessesActivity">
        </activity>
        <activity android:name=".DamageActivity">
        </activity>
        <activity android:name=".EmptyActivity">
        </activity>
        <activity android:name=".SelectableListActivity">
        </activity>
        <activity android:name=".TyrePressure">
        </activity>
        <activity android:name=".CaptureMileageActivity">
        </activity>
        <activity android:name=".CaptureFuelActivity">
        </activity>
        <activity
            android:name=".StartInfleetActivity_"
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".CarInfleetTabsActivity_">
        </activity>
        <activity android:name=".StationActivity">
        </activity>
        <activity android:name=".CarInfoInfleetActivity_">
        </activity>
        <activity
            android:name=".PhotoListActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".DamageListActivity">

        </activity>
    </application>

</manifest>